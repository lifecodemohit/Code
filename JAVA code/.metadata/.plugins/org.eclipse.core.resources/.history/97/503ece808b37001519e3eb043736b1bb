
public class Book {
	String bookTitle;
	String authorName;
	String isbn;
	int price;
	String category;
	
	public Book(String bt, String auth, String isbn_, int pri, String cate){
		bookTitle = bt;
		authorName = auth;
		isbn = isbn_;
		price = pri;
		category = cate;		
	}
	public void setTitle(String bt){
		bookTitle = bt;
	}
	public void setAuthorName(String auth){
		authorName = auth;
	}
	public static void main(String[] args){
		
	}
	
}
