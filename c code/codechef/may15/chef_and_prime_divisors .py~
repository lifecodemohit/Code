def primes(n):
    primfac = []
    d = 2
    x = 1
    while n >= d*d:
        while (n % d) == 0:
            if d not in primfac:
                primfac.append(d)
                x*=d
            n //= d
        d += 1
    if n > 1:
       primfac.append(n)
       x*=n
    return x

t = input()
while(t>0):
	t -= 1
	z = raw_input()
	zx = z.split()
	a = int(zx[0])
	b = int(zx[1])
	x = primes(b)
	flag = 0
"""		
	for i in x:
		if a%i!=0:
			flag = 1
			break
"""
	if (a%x==0):
		print "Yes"
	if(flag==1):
		print "No"
	else:
		
